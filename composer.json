{
    "name": "lechpat/workflow",
    "description": "Workflow plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/lechpat/Workflow",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Valdez",
            "email": "lechpat@me.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "zetacomponents/workflow": "^1.5",
        "zetacomponents/workflow-database-tiein": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Workflow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Workflow\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
